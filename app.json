{
  "expo": {
    "name": "Adwaita Educare",
    "slug": "adwaitaAppV1",
    "privacy": "unlisted",
    "description": "An app to solve all your parenting problems",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adwaitaeducare.parenting"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.adwaitaeducare.parenting"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
